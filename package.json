{
    "name": "mr-src-mangadex",
    "sideEffects": false,
    "keywords": [
        "mangaright-source"
    ],
    "browserlists": [
        "last 2 iOS major versions"
    ],
    "scripts": {
        "build": "npx webpack",
        "build-dev": "npx webpack --mode development",
        "flow": "npx flow-bin",
        "lint": "eslint ./src",
        "format": "eslint --format ./src",
        "repl": "npx webpack --mode development && node ./repl.js"
    },
    "devDependencies": {
        "@babel/preset-flow": "^7.27.1",
        "@babel/preset-env": "^7.28.5",
        "babel-loader": "^10.0.0",
        "babel-plugin-named-asset-import": "^0.3.8",
        "eslint": ">=9.0.0",
        "eslint-plugin-ft-flow": "^3.0.11",
        "flow-bin": "*",
        "globals": "^16.5.0",
        "hermes-eslint": "^0.33.1",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "core-js": ">=3.47.0",
        "node-polyfill-webpack-plugin": "^4.1.0"
    }
}
